// WS handler
.webrepl.run: {
    spawn {
        ws: reagent[`listener;"0.0.0.0:5100";{0N!"ws listener dropped"}];
        react[{[x:ws]
            spawn[{[sock]
                h: reagent[`ws;sock];
                react[{[x:h]
                    r:format["%";@[{eval parse x};"c"$x;{"c"$x}]];
                    b:"x"$r;
                    $[150<#b;
                        h[("c"$50#b),".."];
                        h[r]]
                }]
            };x]
        }]
    }
};